{
    "runtime": "python3.9",
    "handler": "lambda_function.lambda_handler",
    "timeout": 25,
    "memory_size": 256,
    "environment_variables": {
        "PASSPHRASE": "",
        "REDIS_HOST": "",
        "REDIS_KEY": "",
        "REDIS_PORT": 6379,
        "START_MESSAGE": "Welcome to PTBQ.",
        "TOKEN": ""
    },
    "vpc_config": {
        "subnet_ids": [
            ""
        ],
        "security_group_ids": [
            ""
        ]
    },
    "layers": [
        "python-redis",
        "python-telegram-bot"
    ],
    "iam_permissions": {
        "networkPermissions": {
            "effect": "Allow",
            "actions": [
                "ec2:DescribeNetworkInterfaces",
                "ec2:CreateNetworkInterface",
                "ec2:DeleteNetworkInterface"
            ],
            "resources": [
                "*"
            ]
        }
    }
}