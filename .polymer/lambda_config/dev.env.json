{
    "runtime": "python3.9",
    "handler": "lambda_function.lambda_handler",
    "timeout": 25,
    "memory_size": 256,
    "environment_variables": {
        "PASSPHRASE": "",
        "REDIS_HOST": "redis-cluster-endpoint.amazonaws.com",
        "REDIS_KEY": "YourRedisKey",
        "REDIS_PORT": 6379,
        "START_MESSAGE": "Welcome to my quiz bot!",
        "TOKEN": "12345678:12345678"
    },
    "vpc_config": {
        "subnet_ids": [
            "subnet-12345678"
        ],
        "security_group_ids": [
            "sg-12345678"
        ]
    },
    "layers": [
        "python-redis",
        "python-telegram-bot"
    ],
    "iam_permissions": {
        "networkPermissions": {
            "effect": "Allow",
            "actions": [
                "ec2:DescribeNetworkInterfaces",
                "ec2:CreateNetworkInterface",
                "ec2:DeleteNetworkInterface"
            ],
            "resources": [
                "*"
            ]
        }
    }
}